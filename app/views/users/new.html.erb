<section class="todoapp">
  <header class="header">
    <h1>Signup!</h1>

    <%= form_for @user do |f| %>

      <%= f.text_field :email, class: "new-todo", placeholder: "Register with your Email!!!" %>
      <%= f.text_field :password, class: "new-todo", placeholder: "Choose a password" %>
      
      <%= f.submit %>

    <% end %>




  </header>

  <footer class="footer">
  </footer>
</section>
